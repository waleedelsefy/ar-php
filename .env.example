# Environment Configuration Template

# Copy this file to .env and fill in your API keys
# Never commit .env file to version control!

# ==============================================
# AI Providers API Keys
# ==============================================

# Google Gemini
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude
# Get your key from: https://console.anthropic.com/account/keys
CLAUDE_API_KEY=your_claude_api_key_here

# ==============================================
# Free AI Models (Optional)
# ==============================================

# Hugging Face (Free)
# Get token from: https://huggingface.co/settings/tokens
HUGGINGFACE_TOKEN=your_hf_token_here

# Cohere (Free Tier)
# Get key from: https://dashboard.cohere.com/api-keys
COHERE_API_KEY=your_cohere_key_here

# DeepSeek (Free API)
# Get key from: https://platform.deepseek.com
DEEPSEEK_API_KEY=your_deepseek_key_here

# Mistral models can run locally (no key needed)

# ==============================================
# Model Configuration (Optional)
# ==============================================

# Default Models
DEFAULT_GEMINI_MODEL=gemini-pro
DEFAULT_OPENAI_MODEL=gpt-4
DEFAULT_CLAUDE_MODEL=claude-3-opus

# Model Parameters
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2000
AI_TOP_P=0.9

# ==============================================
# Application Settings
# ==============================================

# Environment: development, staging, production
APP_ENV=development

# Enable debug mode
DEBUG=true

# Cache directory
CACHE_DIR=storage/cache

# Log level: debug, info, warning, error
LOG_LEVEL=debug
